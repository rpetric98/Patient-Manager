<form method="post" asp-controller="FileUpload" asp-action="Index" enctype="multipart/form-data">
    <div class="form-group">
        <div class="col-md-10">
            <p>Upload file</p>
            <input type="file" name="files" multiple/>
        </div>
    </div>
    <div class="form-group">
        <div class="col-md-10">
            <input type="submit" value="Upload"/>
        </div>
    </div>
</form>
